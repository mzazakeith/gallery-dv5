    agent any

    tools {
            nodejs 'nodejs'
        }

    stages {
        stage('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }

         stage('Build') {
                    steps {
                        sh 'npm start'
                    }
                }
    }

